  "exports": {
    ".": {
      "browser": "./build/web/index.js",
      "node": {
        "webpack": "./build/web/index.js",
        "default": "./node-entry.js"
      },
      "react-native": {
        "default": "./node-entry.js",
        "types": "./build/types-code/index.d.ts"
      }
    },
